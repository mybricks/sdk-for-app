{
  "name": "@mybricks/sdk-for-app",
  "version": "1.0.25",
  "description": "SDK for application UI",
  "scripts": {
    "build:api": "npm run clean && tsc --build tsconfig.json",
    "clean": "npx rimraf tsconfig.tsbuildinfo ./src/*.js ./src/**.js ./src/**/*.js ./src/**/**/*.js ./src/**/**/**/*.js ./src/api/*.d.ts ./src/api/**.d.ts ./src/api/**/*.d.ts ./src/api/**/**/*.d.ts ./src/api/**/**/**/*.d.ts ./src/decorator/**/**/**/*.d.ts ",
    "clean:temp": "rm -rf .temp",
    "generate:npm": "mkdir .temp && cp package.json ./.temp/package.json && cp -r src/** ./.temp && cd .temp && npm publish",
    "generate:npm-beta": "mkdir .temp && cp package.json ./.temp/package.json && cp -r src/** ./.temp && cd .temp && npm publish --tag beta --registry=https://registry.npmjs.org --access=public",
    "publishToNpm": "npm run build:api && npm run generate:npm && npm run clean && npm run clean:temp",
    "publishToNpm:beta": "npm run build:api && npm run generate:npm-beta && npm run clean && npm run clean:temp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mybricks/sdk-for-app.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "api",
    "ui",
    "decorator",
    "package.json"
  ],
  "author": "CheMingjun",
  "license": "ISC",
  "peerDependencies": {
    "axios": ">=1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@mybricks/rxui": "^1.0.86",
    "@types/dom-to-image": "^2.6.4",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/parser": "^5.41.0",
    "antd": "^5.0.2",
    "axios": "^1.2.0",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "core-js": "^3.23.5",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "8.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.2.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "dom-to-image": "^2.6.0",
    "form-data": "^4.0.0",
    "html-to-image": "^1.11.11"
  }
}
